@model QTHT.Models.View.StationConfigModel

<div class="modal fade over-flow-hidden" id="exportStation-@Model.StationConfig.ID">
    <div class="modal-dialog w-600px">
        <form title="Xác nhận xuất excel" method="post" asp-controller="StationConfig" asp-action="Export">
            <div class="modal-content">
                <div class="modal-header title-form">
                    <h4 class="modal-title white" id="">
                        Xác nhận xuất dữ liệu
                    </h4>
                </div>
                <div class="modal-body">
                    <div class="container-fluid">
                        <div class="row form-group has-feedback w-100">
                            <label class="col-md-3 control-label">
                                Tên file xuất
                                <small class="red">(*)</small>
                            </label>
                            <div class="col-md-9">
                            <input type="text"
                                class="form-control w-100"
                                data-bv-field="FileName"
                                data-bv-notempty="true"
                                data-bv-lessthan-value="31"
                                data-bv-lessthan-message="Tên file xuất không quá 31 kí tự"
                                data-bv-notempty-message="Tên file xuất không được trống"
                                value=""
                                placeholder="Tên file xuất không quá 31 kí tự"
                                maxlength="31"
                                name="FileName" />
                                <input type="hidden" value="@ViewBag.ID" name="id" />
                            </div>
                        </div>
                        <div class="row form-group">
                            <label class="col-md-2 control-label">
                            </label>
                            <div class="col-md-10" id="formExportView">

                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-danger" data-bs-dismiss="modal">Huỷ</button>
                    <button type="submit" class="btn btn-primary" data-submit="modal">Xuất excel</button>
                </div>
            </div>
        </form>
    </div>
</div>